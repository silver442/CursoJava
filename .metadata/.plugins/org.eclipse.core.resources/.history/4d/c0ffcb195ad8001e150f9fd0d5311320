package hernandez.silver.ejerciciospoo;

import java.util.Random;

public class UsoGeneradorContrase√±a {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Random rnd = new Random();
		
		//System.out.println((char)rnd.nextInt(91));
		
		//char caracter=(char)(rnd.nextInt(91) + 65);
		
		char caracter;
		
		System.out.println();
		
		//System.out.println(caracter);
		for(int i=0;i<7;i++) {
			caracter=(char)rnd.nextInt(91);
			if(caracter<10 || caracter>65) {
				
				System.out.println(caracter);
				
			}else {
				i=i;
			}
		}
	}
}
