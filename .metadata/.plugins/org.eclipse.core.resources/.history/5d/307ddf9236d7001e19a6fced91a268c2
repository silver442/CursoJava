package hernandez.silver.ejerciciospoo;

public class Colegio {
	
	public Colegio(String nombreColegio, int cantidadAlumnos) {
		this.nombreColegio=nombreColegio;
		
		this.cantidadAlumnos=cantidadAlumnos;
	}
	
	public void nuevoAlumno(String nombreAlumno, double nota) {
		
		Alumno[] alumnos=new Alumno[cantidadAlumnos];
		
	}
	
	
	private final String nombreColegio;

	private int cantidadAlumnos;
}
