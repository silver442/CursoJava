package hernandez.silver.ejerciciospoo;

import java.util.Random;

public class UsoGeneradorContrase√±a {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Random rnd = new Random();
		
		//System.out.println((char)rnd.nextInt(91));
		
		//char caracter=(char)(rnd.nextInt(91) + 65);
		
		int numAleatorio;
		
		char caracter;
		System.out.println();
		
		//System.out.println(caracter);
		for(int i=0;i<7;i++) {
			numAleatorio=rnd.nextInt(123);
			if((numAleatorio<10 || numAleatorio>65 || numAleatorio<123 || numAleatorio>96) ) {
				
				if(numAleatorio<10) {
					
					System.out.println(numAleatorio);
				}else {
					
					caracter=(char)numAleatorio;
					System.out.println(caracter);
				}
				
			}else {
				i--;
			}
		}
	}
}
