		Empleado empleados[] = new Empleado[4];
		EmpleadoAsalariado empsal1 =  new EmpleadoAsalariado("6546546Z", "Antonio", "LÃ³pez", 2014, 1125);
		empleados[0] = empsal1;
		
		EmpleadoComision empcom1=new EmpleadoComision("7879879S","Sandra", "Nieto", 2011, 169, 7.10); 
