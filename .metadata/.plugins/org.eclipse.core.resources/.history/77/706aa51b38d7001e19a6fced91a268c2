package hernandez.silver.ejerciciospoo;

public class Alumno {

	public Alumno(String nombreAlumno, double nota) {
		
		this.nombreAlumno=nombreAlumno;
		
		this.nota=nota;
		
	}
	
	// Metos getter
	public String getAlumno() {
		
		return nombreAlumno;
		
	}
	
	private final String nombreAlumno;
	
	private double nota;
}
