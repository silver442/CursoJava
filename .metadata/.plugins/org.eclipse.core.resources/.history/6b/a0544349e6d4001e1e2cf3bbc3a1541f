package hernandez.silvestre.flujo;

public class Ejemplo_For {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		for(int i=0;i<20;i++) {
			System.out.println("Aviso: " + i); // Lo repetira 10 veces
		
			if(i==6)break;
		}
		
		System.out.println("has salido del bucle");
	}
}
